<body>
<div id="header-wrapper">
	<div id="header" class="container">
		<div id="logo">
			<h1><span class="fa fa-bug"></span><a href="/">Words</a></h1>
		</div>
		<div id="menu">
			<ul>
				<li class="current_page_item"><a href="/" accesskey="1" title="">Homepage</a></li>
			</ul>
		</div>
	</div>
</div>
<div id="header-featured">
	<div id="banner-wrapper">
		<div id="banner" class="container">
			<h2>Welcome to Words</h2>
			<form method="post" action="upload-file" name="file_upload" enctype="multipart/form-data">
				<p id="file_chosen">No file chosen</p>
				<div class="upload">Choose a File</div><br>
				<input id="file" name="fileForm" class="hide" type="file"/>
				<br><br>
				<input type="submit" name="upload" value="Upload Your File" class="button" id="submit">
			</form>
			<!--<a href="#" class="button">Upload Your File</a>-->
		</div>
	</div>
</div>
<script>
	$(document).on("click", ".upload", function(){
		$("#file").click();
	});
</script>
<script>
	$("#file").change(function(){
		$("#file_chosen").html($("#file").val());
	});
</script>
